    def compute(self, x, y):
        result = (x + y) * 3.14 - 2 ** 5
        result /= 10
        result %= 7
        result //= 2
        result += 1
        result -= 2
        return result

def factorial(n):
    # factorial con recursion
    if n <= 1:
        return 1
    else:
        return n * factorial(n-1)

